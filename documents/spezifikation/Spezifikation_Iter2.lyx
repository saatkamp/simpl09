	referenceType="onInstantiation | fresh |
\end_layout

\begin_layout Plain Layout

	periodic | external" 
\end_layout

\begin_layout Plain Layout

	period="duration"? 
\end_layout

\begin_layout Plain Layout

	external="partnerLink"? />*
\end_layout

\begin_layout Plain Layout

\begin_inset Caption

\begin_layout Plain Layout
\begin_inset CommandInset label
