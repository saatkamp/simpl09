<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             version="1.0">
    <persistence-unit name="simpl-dao">
        <!-- 
            This properties file is used specifically by the
            OpenJPA Enhancer.
         -->
        <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
        <class>org.apache.ode.dao.simpl.ActivityRecoveryDAOImpl</class>
        <class>org.apache.ode.dao.simpl.CorrelationSetDAOImpl</class>
        <class>org.apache.ode.dao.simpl.CorrelatorDAOImpl</class>
        <class>org.apache.ode.dao.simpl.EventDAOImpl</class>
        <class>org.apache.ode.dao.simpl.FaultDAOImpl</class>
        <class>org.apache.ode.dao.simpl.MessageDAOImpl</class>
        <class>org.apache.ode.dao.simpl.MessageExchangeDAOImpl</class>
        <class>org.apache.ode.dao.simpl.MessageRouteDAOImpl</class>
        <class>org.apache.ode.dao.simpl.PartnerLinkDAOImpl</class>
        <class>org.apache.ode.dao.simpl.ProcessDAOImpl</class>
        <class>org.apache.ode.dao.simpl.ProcessInstanceDAOImpl</class>
        <class>org.apache.ode.dao.simpl.ScopeDAOImpl</class>
        <class>org.apache.ode.dao.simpl.XmlDataDAOImpl</class>
        <class>org.apache.ode.dao.simpl.CorrSetProperty</class>
        <class>org.apache.ode.dao.simpl.MexProperty</class>
        <class>org.apache.ode.dao.simpl.XmlDataProperty</class>
    </persistence-unit>
</persistence>