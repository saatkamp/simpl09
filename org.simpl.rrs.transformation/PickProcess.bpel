<!-- PickProcess BPEL Process [Generated by the Eclipse BPEL Designer] -->
<bpel:process name="PickProcess"
         targetNamespace="http://eclipse.org/bpel/sample"
         suppressJoinFailure="yes"
         xmlns:tns="http://eclipse.org/bpel/sample"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="PickProcessArtifacts.wsdl" namespace="http://eclipse.org/bpel/sample" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:PickProcess"
                     myRole="PickProcessProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:PickProcessRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:PickProcessResponseMessage"/>
        <bpel:variable name="clientRequest" messageType="tns:workRequest"></bpel:variable>
        <bpel:variable name="clientResponse" messageType="tns:workResponse"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in PickProcess.wsdl 
             -->
        
        
        <!-- Generate reply to synchronous request -->
        <bpel:pick name="Pick" createInstance="yes">
            <bpel:onMessage partnerLink="client" operation="process" portType="tns:PickProcess" variable="input">
                <bpel:sequence>
                    <bpel:assign validate="no" name="Assign">
                        <bpel:copy>
                            <bpel:from>
                                <bpel:literal xml:space="preserve"><tns:PickProcessResponse xmlns:tns="http://eclipse.org/bpel/sample" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>process</tns:result>
</tns:PickProcessResponse></bpel:literal>
                            </bpel:from>
                            <bpel:to part="payload" variable="output"></bpel:to>
                        </bpel:copy>
                    </bpel:assign>
                    <bpel:reply name="Reply" partnerLink="client" operation="process" portType="tns:PickProcess" variable="output"></bpel:reply>
                </bpel:sequence>
            </bpel:onMessage>
            <bpel:onMessage partnerLink="client" operation="work" portType="tns:PickProcess" variable="clientRequest">
                <bpel:sequence>
                    <bpel:assign validate="no" name="Assign1">
                        
                        <bpel:copy>
                            <bpel:from>
                                <bpel:literal xml:space="preserve"><tns:workResponse xmlns:tns="http://eclipse.org/bpel/sample" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:out>work</tns:out>
</tns:workResponse></bpel:literal>
                            </bpel:from>
                            <bpel:to part="parameters" variable="clientResponse">
                            </bpel:to>
                        </bpel:copy>
                    </bpel:assign>
                    <bpel:reply name="Reply1" partnerLink="client" operation="work" portType="tns:PickProcess" variable="clientResponse"></bpel:reply>
                </bpel:sequence>
            </bpel:onMessage>
        </bpel:pick>
        
    </bpel:sequence>
</bpel:process>

